include<stdio.h>
int main (){
        int arr[10]={3,2,7,8,0,4,6,5,1,9};
        int i,j;
        for(i=0;i<9;i++){
                for(j=1+i;j<=9;j++){
                        if(arr[i]>arr[j]){
                                int tmp=arr[i];
                                arr[i]=arr[j];
                                arr[j]=tmp;
                        }
                }
        }
        for (i=0;i<=9;i++){
                printf("%d\t",i);
        }
        return 0;
}
