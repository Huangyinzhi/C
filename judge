#include<stdio.h>
int main(){
        int n,i,j=0;
        printf("please enter a positive integer: ");
        scanf ("%d",&n);

        for(i=2;i<=n/2;i++)
            {
                if(n%i==0)
              {  j=1;
                 break;
              }
            }
        if(1==n)
        {  printf("1 is neither a prime nor a composite number.");}
        else
        { if(j==0)
          {printf("Yes");}
          else
          {printf("no");}

        }
                   return 0;
}
