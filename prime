#include<stdio.h>
int main(){
        int n,i,j,k=0;
        printf("Please enter a positive integer:");
        scanf("%d",&n);

        for (i=1;i<=n;i++)
        { for (j=2;j<=i/2;j++)
                { if (i%j==0)
                        {k=1;
                        break;
                       }
                 }
          if (k==0)
          {printf("%d\n",i);}
          else
          { k=0;}

        }
        return 0;
}                               
